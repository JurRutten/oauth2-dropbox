{
    "name": "pixelfear/oauth2-dropbox",
    "description": "Dropbox OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Varga",
            "email": "jason@pixelfear.com",
            "homepage": "https://github.com/pixelfear"
        },
        {
            "name": "Steven Maguire",
            "email": "stevenmaguire@gmail.com",
